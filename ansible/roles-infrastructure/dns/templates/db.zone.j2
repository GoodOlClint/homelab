$TTL 1h
@   IN  SOA  ns1.{{ zone_name }}. admin.{{ zone_name }}. (
        {{ serial | default('2025061201') }} ; serial
        1h         ; refresh
        15m        ; retry
        1w         ; expire
        1h )       ; minimum

    IN  NS  ns1.{{ zone_name }}.
ns1 IN  A   {{ zone_dns_ip }}

dns IN  A   {{ zone_dns_ip }}